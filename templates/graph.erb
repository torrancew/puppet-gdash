title       '<%= @name %>'
vtitle      '<%= @vtitle %>'
description '<%= @description %>'

<%- @data['fields'].each do |f| %>
field <%= f['name'].to_s.intern %>,
  :scale => <%= f['scale'].to_f %>,
  :color => '<%= f['color'] %>',
  :alias => '<%= f['alias'] %>',
  :data  => '<%= f['data'] %>'

<%- end %>
